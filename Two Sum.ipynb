    "\n",
    "nums = [2,7,11,15]\n",
    "target = 9\n",
    "def twosum(nums, target):\n",
    "    hmap = {}\n",
    "    for i in range(len(nums)):\n",
    "        complement = target - nums[i]\n",
    "        if nums[i] in hmap:\n",
    "            return [hmap[nums[i]], i]\n",
    "        else:\n",
    "            hmap[complement] = i\n",
    "\n",
    "twosum(nums, target)\n",
    "        "
   ]
